<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/editText3"
        android:layout_width="287dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Health & Wellness"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="364dp"
        android:layout_height="41dp"
        android:layout_marginBottom="8dp"
        android:background="#f29e9e"
        android:text="Medication"
        android:textAlignment="center"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        tools:layout_editor_absoluteX="11dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="365dp"
        android:layout_height="40dp"
        android:layout_marginBottom="8dp"
        android:background="#f29e9e"
        android:text="Health Records"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/button4"
        tools:layout_editor_absoluteX="10dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="365dp"
        android:layout_height="43dp"
        android:background="#f29e9e"
        android:text="Medical I.D."
        android:textSize="23sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="521dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="174"
        android:layout_height="150dp"
        app:srcCompat="@drawable/bi"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="95dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:background="yellow"
        android:text="Exercise"
        android:textAllCaps="false"
        android:textColor="#800000"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="216dp" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="174"
        android:layout_height="150"
        app:srcCompat="@drawable/bg"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="95dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#558336"
        android:text="Analysis"
        android:textColor="@android:color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="216dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="72dp"
        android:layout_height="58dp"
        app:srcCompat="@drawable/st"
        tools:layout_editor_absoluteX="286dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="174"
        android:layout_height="157dp"
        app:srcCompat="@drawable/n2"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="254dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#ec6945"
        android:text="Nutrition"
        android:textColor="#800000"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="381dp" />

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="174"
        android:layout_height="157"
        app:srcCompat="@drawable/sl"
        tools:layout_editor_absoluteX="201dp"
        tools:layout_editor_absoluteY="254dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#b1c1e1"
        android:text="Sleep"
        android:textColor="dark @android:color/holo_blue_dark"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="267dp"
        tools:layout_editor_absoluteY="381dp" />

</android.support.constraint.ConstraintLayout>